{
	"name": "eemmy/emmyvc",
	"description": "MVC framework similar to laravel",
	"license": "GPL",
	"support": {
		"issues": "https://github.com/eEmmy/EmmyVC/issues/",
		"source": "https://github.com/eEmmy/EmmyVC/"
	},
	"author": {
		"name": "Emmy Gomes",
		"email": "aou-emmy@outlook.com"
	},
	"require": {
		"php": "^7.2|^8.0",
		"phpmailer/phpmailer": "~6.1",
		"matthiasmullie/minify": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"DB\\": "src/Database/",
			"App\\Http\\": "App/Http/",
			"TemplateEngine\\": "src/TemplateEngine/",
			"App\\Http\\Middleware\\": "App/Http/Middlewares/",
			"App\\Http\\Controller\\": [
				"App/Http/Controllers/", 
				"App/Http/Controllers/Auth/"
			],
			"App\\": [
				"Bootstrap/",
				"App/Models/",
				"src/Mail/",
				"src/Auth/"
			]
		},
		"files": [
			"Configuration/auth.php",
			"Configuration/url.php",
			"Configuration/database.php",
			"Configuration/mail.php",
			"Configuration/reporting.php",
			"Configuration/session.php",
			"Routes/web.php",
			"Bootstrap/App.php"
		]
	}
}